{{ ctf.domain_name }} {
    reverse_proxy web:3000
    encode gzip zstd

    header {
        X-Role "player"
    }
}

admin.{{ ctf.domain_name }} {
    reverse_proxy admin:3000
    encode gzip zstd

    header {
        X-Role "admin"
    }
}

api.{{ ctf.domain_name }} {
    reverse_proxy oxide:3000
    encode gzip zstd
}
