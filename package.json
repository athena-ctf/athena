{
  "name": "frontend",
  "private": true,
  "scripts": {
    "ui:add": "pnpm --filter @repo/ui ui:add",
    "generate:openapi": "pnpm --filter @repo/api generate",
    "generate:emails": "pnpm --filter @repo/emails export",
    "biome": "biome check .",
    "biome:fix": "biome check . --write",
    "storybook:web": "pnpm --filter web storybook",
    "storybook:admin": "pnpm --filter admin storybook"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2"
  },
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=18"
  }
}
